#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/nio/Desktop/workspace/inQbator-Web/server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/nio/Desktop/workspace/inQbator-Web/server/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../google-p12-pem@4.0.1/node_modules/google-p12-pem/build/src/bin/gp12-pem.js" "$@"
else
  exec node  "$basedir/../../../../../google-p12-pem@4.0.1/node_modules/google-p12-pem/build/src/bin/gp12-pem.js" "$@"
fi
